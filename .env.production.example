# =============================================================================
# UNMAPPED OS - PRODUCTION ENVIRONMENT (TEMPLATE)
# =============================================================================
# Copy these values to Vercel Dashboard → Settings → Environment Variables
# DO NOT COMMIT REAL SECRETS.
# =============================================================================

# NextAuth
NEXTAUTH_SECRET=
NEXTAUTH_URL=

# App URLs
NEXT_PUBLIC_APP_URL=
NEXT_PUBLIC_DOMAIN=
NODE_ENV=production

# Supabase
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=

# OAuth Providers
NEXT_PUBLIC_GOOGLE_ENABLED=false
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

NEXT_PUBLIC_GITHUB_ENABLED=false
GITHUB_ID=
GITHUB_SECRET=

NEXT_PUBLIC_APPLE_ENABLED=false
APPLE_CLIENT_ID=
APPLE_TEAM_ID=
APPLE_KEY_ID=
APPLE_PRIVATE_KEY=

# Map Provider
NEXT_PUBLIC_MAP_PROVIDER=maplibre
# Recommended free default for MapLibre (no key required):
NEXT_PUBLIC_MAPLIBRE_STYLE_URL=https://demotiles.maplibre.org/style.json

# Optional Mapbox
NEXT_PUBLIC_MAPBOX_TOKEN=
NEXT_PUBLIC_MAPBOX_STYLE=mapbox://styles/mapbox/dark-v11

# Optional feature flags
FEATURE_GAMIFICATION=true
FEATURE_ACTIVITY_EXPORT=true
FEATURE_OPERATIVE_MEMORY=true
FEATURE_CRISIS_MODE=true
FEATURE_SHADOW_MODE=true
FEATURE_GHOST_BEACONS=true
FEATURE_WHISPER_ENGINE=true
FEATURE_TEXTURE_SYSTEM=true

# Optional intelligence
TEXTURE_UPDATE_INTERVAL=3600000
